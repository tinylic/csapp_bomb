//testing expression to chart

//check sample
CHECK_EQUAL(expr_to_truthtable(3, "~A | B & C"), "11010101");

//check unit input
CHECK_EQUAL(expr_to_truthtable(1, "A"), "10");
CHECK_EQUAL(expr_to_truthtable(2, "A"), "1010");

//check complex input
CHECK_EQUAL(expr_to_truthtable(4, "A & ~B ^ C & D"),
		"1101001000100010");

